{
    "name": "akamai-edgeworker-testutils",
    "version": "1.0.12",
    "main": "test_utils.js",
    "type": "module",
    "devDependencies": {
        "edgeworkers-jest-mocks": "^1.0.28",
        "jest": "^29.7.0",
        "@babel/cli": "^7.25.6",
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.4"
    },
    "scripts": {
        "test": "jest"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
        ],
        "coverageThreshold": {
            "global": {
                "lines": 80
            }
        },
        "transformIgnorePatterns": [
            "node_modules/(?!akamai-edgeworker-testutils|edgeworkers-jest-mocks/__mocks__)"  
        ],
        "moduleDirectories": [
            "node_modules",
            "node_modules/edgeworkers-jest-mocks/__mocks__"
        ]
    }


  }
  
